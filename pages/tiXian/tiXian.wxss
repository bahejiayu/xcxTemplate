.tiContainer{
  padding: 0 40rpx;
}
.tiInp{
  height: 105rpx;
  border-bottom: 2rpx solid #EAEAEA;
  position: relative;
  padding-left:175rpx ;
  line-height: 105rpx;
  padding-right: 80rpx;
}
.tiInpTitle{
  font-size:32rpx ;
  color: #222222;
  position: absolute;
  left: 0;
  top: 0;
}
.tiInp input{
  height: 105rpx;
  font-size:30rpx;
  color: #222;
  width: 100%;
}
.tiInp .icon{
  color: #222;
  font-size: 28rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.paymentBox{
  padding: 30rpx 0 20rpx 0;
}

.paymentTit{
  font-size: 34rpx;
  color: #222222;
  margin-bottom: 20rpx;
}
.paymentInp{
  padding-left: 40rpx;
  position: relative;
  height: 41rpx;
  padding-bottom: 14rpx;
  border-bottom: 2rpx solid #EAEAEA;
}

.paymentInpL{
  position: absolute;
  left: 0;
  font-size:32rpx;
  color: #222;
  top: 50%;
  transform: translateY(-60%);
}
.paymentInpL input{
  width: 100%;
  height: 100%;
}
.inpStyle{
  font-size: 32rpx;
}
.paymentInp input{
  font-size: 52rpx;
}
.bServie{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bServie view:first-child{
  font-size: 28rpx;
  color: #A2A2A2;
}
.bServie view:last-child{
  font-size: 28rpx;
  color: #222;
}
.depositFix{
  position: fixed;
  padding: 0 40rpx;
  padding-bottom: 40rpx;
  width: 100%;
  box-sizing: border-box;
  bottom: 0;
}
.isOk{
  color: #fff;
  background-color:#457AE3!important ;
}
.fixAcc{
  position: fixed;
  right: 40rpx;
  bottom: 20%;
  border-radius: 50%;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 28rpx;
  background-color:#457AE3!important ;
  width: 120rpx;
  height: 120rpx;
  flex-direction: column;

}