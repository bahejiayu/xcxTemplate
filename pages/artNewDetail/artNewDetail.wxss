.artDetail{
    padding: 14rpx 40rpx;

}
.artDetailTit{
  font-size:34rpx ;
  color: #222222;
  line-height:48rpx ;
  font-weight: bold;
}
.artDate{
  display: flex;
  margin-top: 10rpx;
}
.artDate view{
  font-size: 28rpx;
  color: #A2A2A2;
  margin-bottom: 15rpx;
}
.artDate view:first-child{
  padding-right: 22rpx;
}
.artContent{
  font-size:30rpx ;
  color: #696F7C;
  line-height: 50rpx;
  padding-bottom: 110rpx;
}
.orderFix{
  position: fixed;
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
  height: 100rpx;
  background-color: #fff;
  z-index: 100;
  bottom: 0;
  /* box-shadow: 0px -5rpx 6rpx  #EAEAEA; */
  box-shadow:0px 0px 20px 0px rgba(0,0,0,0.1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.circuseeBoxL{
  display: flex;
  align-items: center;
  height: 100rpx;
}
.cirImgs{
    position: relative;
    height: 100%;
}
.cirImgItem{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 0;
    height: 56rpx;
    width: 56rpx;
    box-sizing: border-box;
    border: 3rpx solid #fff;
    border-radius: 50%;
    overflow: hidden;

}
.cirImgItem:nth-of-type(2){
  left: 30rpx;
}
.cirImgItem:nth-of-type(3){
  left: 60rpx;
}
.cirImgItem:nth-of-type(4){
  left: 90rpx;
}
.cirImgItem:nth-of-type(5){
  left: 120rpx;
}
.cirImgItem image{
  width: 100%;
  height: 100%!important;  
}
.circuseeTxt{
  font-size: 32rpx;
  color: #94959C;
  /* padding-left: 160rpx; */
}
.artFixIcon view{
  font-size: 20rpx;
  color: #222222;
  margin-bottom: 4rpx;
}
.artIcon image{
  width: 40rpx;
}
.artFixIcon{
  text-align: center;
  margin-left: 40rpx;
}
.artFixR{
  display: flex;
}