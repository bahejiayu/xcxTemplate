.sellNav{
  padding: 0 40rpx;
}

.libraryNavList{
  display: flex;
  align-items: center;
  height: 76rpx;
  justify-content: space-between;
  border-bottom: 2rpx solid #EAEAEA;

}
.libraryNavItem{
  font-size: 34rpx;
  color: #A2A2A2;
}
.libraryNavItem text{
  line-height: 70rpx;
  display: inline-block;
  position: relative;
}
.libraryNavItem text.cur{
  color: #222222;
  font-weight: bold;
  /* border-bottom:5rpx solid #222222 ; */
}
.libraryNavItem text.cur::before{
  position: absolute;
  height: 5rpx;
  width: 64rpx;
  bottom: 0;
  background-color: #222222;
  content: '';
  left: 50%;
  transform: translateX(-50%);
}

.orderList{
  padding:  40rpx;
  padding-bottom: 0;
}


.booksItem{
  position: relative;
  height: 322rpx;
  padding-left: 310rpx;
  margin-bottom: 50rpx;
  display: block;
}
.bookItemImg{
  position: absolute;
  width:240rpx ;
  height: 322rpx;
  left: 0;
  top: 0;
}
.bookItemImg image{
  width: 100%;
  height: 100%!important;

}
.bookInfoName{
  font-size:34rpx ;
  color: #000000;
  margin-bottom: 13rpx;
  position: relative;
  padding-right: 120rpx;
}
.bookWiter{
  font-size: 26rpx;
  color:#222222 ;
  /* padding-right: 140rpx; */
  width: 250rpx;
  position: relative;
}
.shadeView{
  z-index: 100;
}
.buyStyle{
  font-size: 28rpx;
  color: #222222;
}
.bookPublish{
  font-size: 24rpx;
