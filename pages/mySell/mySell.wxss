}
.booksContainer{
  padding:  40rpx;
  padding-bottom: 0;

}
.collectAddBtn{
  width:148rpx ;
  height: 50rpx;
  border: 1px solid #9A141E;
  color: #9A141E;
    border-radius: 8rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;


}
.payEndTime{
  font-size:24rpx ;
  color: #222222;
  padding-top: 10rpx;
}

.againBuy{
  border: 1px solid #A2A2A2;
color: #A2A2A2;

}

.orderStatus{
  position: absolute;
  right: 0;
  top: 0;
  font-size: 30rpx;
  color: #A2A2A2;
}
.orderStatus.cur{
  color: #9A141E;
}
.sellFix {
  position: fixed;
  background-color: #fff;
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
  bottom: 0;
  z-index: 1000;
  transform: translateY(100%);
  transition: all 0.3s;

}

.sellFix.showSell {
  transform: none;
}
.sellStyleTxt{
  font-size: 30rpx;
  color: #9A141E;
}
.sellFixTit {
  padding: 40rpx 0 44rpx 0;
  font-size: 36rpx;
  color: #222;
  position: relative;
  text-align: center;
  border-bottom: 2rpx solid #EAEAEA;

}

.sellFixTit .icon {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  font-size: 36rpx;
  color: #222;
}

.sellFixInp {
  height: 108rpx;
  border-bottom: 2rpx solid #EAEAEA;
  position: relative;
  padding-left: 180rpx;
  line-height: 108rpx;
  padding-right: 40rpx;
}
.sellFixInp .icon{
  position: absolute;
  right: 0;
  top: 50%;
  transform:translateY(-50%);
}

.sellFixInpTit {
  font-size: 32rpx;
  position: absolute;
  left: 0;
  color: #222222;
}

.sellFixInp input {
  width: 100%;
  height: 108rpx;
  text-align: right;
  font-size: 34rpx;
  color: #222222;

}
.tlbw{
  display: flex;
