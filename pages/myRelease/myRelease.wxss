.orderNav{
  padding: 0 40rpx;
}


.libraryNavList{
  display: flex;
  align-items: center;
  height: 76rpx;
  justify-content: space-between;
  border-bottom: 2rpx solid #EAEAEA;

}
.libraryNavItem{
  font-size: 34rpx;
  color: #A2A2A2;
}
.libraryNavItem text{
  line-height: 70rpx;
  display: inline-block;
  position: relative;
}
.libraryNavItem text.cur{
  color: #222222!important;
  font-weight: bold;
  /* border-bottom:5rpx solid #222222 ; */
}
.libraryNavItem text.cur::before{
  position: absolute;
  height: 5rpx;
  width: 64rpx;
  bottom: -2rpx;
  background-color: #222222;
  content: '';
  left: 50%;
  transform: translateX(-50%);
}
