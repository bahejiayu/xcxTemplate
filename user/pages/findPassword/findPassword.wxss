.modifyBox{
  padding: 0 40rpx;
}
.midifyItem{
  padding-left:196rpx ;
  height: 105rpx;
  position: relative;
  padding-right: 160rpx;
  border-bottom: 1px solid #EAEAEA;
  line-height: 105rpx;
}
.getCode.cur{
  background-color: #9A141E;
  color: #fff;
}

.midifyTit{
  position: absolute;
  height: 100%;
  left: 0;
  top: 0;
  font-size: 32rpx;
  color: #222222;
}
.midifyItem input{
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  height: 105rpx;

}
.getCode{
  width:160rpx ;
  height: 54rpx;
  position: absolute;
  right: 0;
  top: 50%;
  font-size: 24rpx;
  color: #BCBCBC;
  background-color: #EAEAEA;
  border-radius: 8rpx;
  transform: translateY(-50%);
}
.quPhone{
  position: absolute;
  padding-left: 24rpx;
  padding-right: 54rpx;
  height: 50rpx;
  top: 50%;
  border-right: 1px solid #EAEAEA;
  line-height: 50rpx;
  left: 0;
  font-size:34rpx ;
  color: #000000;
  transform: translateY(-50%);
}
.righUnfold{
  position: absolute;
  right: 0;
  top: 0;
}
.midifyItem picker{
  height: 100%;
  width: 100%;
}



.addressBtn{
  position: fixed;
  box-sizing: border-box;
  bottom: 40rpx;
  width: 90%;
}





