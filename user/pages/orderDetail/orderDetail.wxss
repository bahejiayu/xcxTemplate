}

.carFixBottom{
  position: fixed;
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
  height: 112rpx;
  background-color: #fff;
  z-index: 100;
  bottom: 0;
  box-shadow: 0px -5rpx 6rpx  #EAEAEA;
  display: flex;
  justify-content: space-between;
  align-items: center;

}

.carFixL{
  display: flex;
  align-items: center;
  font-size: 34rpx;
  color: #A2A2A2;
}
.carFixR{
  display: flex;
