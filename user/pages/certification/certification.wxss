.certificatBox{
  padding:27rpx 40rpx ;
}

.ertiItem{
  margin-bottom: 40rpx;
}
.ertTitle{
  display: flex;
  align-items: center;
  height: 44rpx;
  margin-bottom: 14rpx;
}

.ertTitle text{
  display: block;
  height: 44rpx;
  font-size: 40rpx;
  color: #9A141E;
}
.ertTitle view{
  font-size: 32rpx;
  color: #222222;
  padding-left: 12rpx;
}
.ertInp input{
  width: 100%;
  height:75rpx ;
  background-color: #F9F9F9;
  box-sizing: border-box;
  padding: 0 26rpx;
  font-size:28rpx ;
  border-radius: 8rpx;

}


.erInpStyle{
  color: #BCBCBC;
}

.uploadCardBox{
  height:150rpx;
  padding-right: 80rpx;
  padding-top: 8rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.uoloadCardL  image{
  width: 236rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  height: 146rpx!important;
  transform:translate(-50%,-50%) ;
  
}
.uoloadCardL{
  width:240rpx ;
  height: 150rpx;
  box-sizing: border-box;
  border: 1px dashed #BCBCBC;
  position: relative;
  overflow: hidden;
  border-radius: 8rpx;
}
.uoloadCardL .icon{
  font-size:50rpx ;
  color: #A2A2A2;
  position: relative;
  z-index: 100;
}
.uoloadCardR image{
  width:202rpx ;
}
