text-align:center;

}
.round, button.round, button.round::after, button.icon, tag.line.round::after, button.icon::after {
border-radius:5000px;

}
avatar {
font-variant:small-caps;
line-height:1.5;
color:#ddd;
margin:0;
padding:0;
display:inline-block;
text-align:center;
background:#ccc;
color:#fff;
white-space:nowrap;
position:relative;
width:32px;
height:32px;
background-size:cover;
background-position:center;

}
avatar text {
transform:scale(1.2);
display:inline-block;

}


.avatar.lg {
width:45px;
height:45px;
line-height:45px;
font-size:22px;

}



.shadow-lg {
box-shadow:0px 20px 50px 0px rgba(0, 0, 0, 0.07);

}
.text-bold {
font-weight:bold;

}
.text-xl {
font-size:18px;

}
.margin-tb {
margin-top:15px;
margin-bottom:15px;

}
.text-gray {
color:#aaa;
font-size: 30rpx;

}
.padding {
padding:15px;

