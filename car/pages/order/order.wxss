  align-items: center;
  justify-content: space-between;
  font-size:30rpx ;
  color: #222222;
}
.carItem{
  position: relative;
  padding-bottom: 50rpx;
  margin-top: 50rpx;
  overflow: hidden;
  border-bottom: 1px solid #EAEAEA;
  margin-bottom: 50rpx;

}
.orderGoods .carItem{
  margin-bottom: 0;
}
.carContainer{
  height: 230rpx;
  padding-left:210rpx ;
  position: relative;
  transition: all 0.3s;
}
.carContainer.arrowLeft{
    transform:translateX(-160rpx) ;
}
.carBox{
  padding-top: 30rpx;
  padding: 0 40rpx;
  padding-bottom: 120rpx;
}
.wai_ {

  width:34rpx;
  height:34rpx;
  background-color:#fff;
  border:2.5rpx solid #A2A2A2;
  -webkit-border-radius:50%;
  border-radius:50%;
  vertical-align:middle;
  position: absolute;
  display: inline-block;
  left: 0;
  top: 50%;
  transform:translateY(-50%) ;
  
  }
  .carFixBottom .wai_{
    position: static;
    transform: none;
    margin-right: 12rpx;
  }
  .wai_.cur{
     border-color: #9A141E!important;
      background-color: #9A141E!important;
  }
  .guo_{
      position: absolute;
       left: 8rpx; 
       top: 10rpx; 
      width: 20rpx;
      height: 10rpx;
      border-left: 4rpx solid #fff;
      border-bottom: 4rpx solid #fff;
      -webkit-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
      display: none;
      transform: rotate(-45deg);
  }
  .cur .guo_{
    display: block;
    }
  .goodsImgContainer{
    width:170rpx ;
    height: 230rpx;
    top: 0;
    left: 0rpx;
    position: absolute;
  }
.goodsImgContainer image{
  width: 100%;
  height: 100%;
}
.carContent{
